# Git LFS for large data files
data/layers/*.geojson filter=lfs diff=lfs merge=lfs -text
data/rasters/*.tif filter=lfs diff=lfs merge=lfs -text
data/documents/*.pdf filter=lfs diff=lfs merge=lfs -text
data/synthetic/*.json filter=lfs diff=lfs merge=lfs -text
docs/assets/diagrams/*.png filter=lfs diff=lfs merge=lfs -text
docs/assets/diagrams/*.jpg filter=lfs diff=lfs merge=lfs -text
docs/assets/diagrams/*.webp filter=lfs diff=lfs merge=lfs -text
*.png filter=lfs diff=lfs merge=lfs -text
*.pdf filter=lfs diff=lfs merge=lfs -text
