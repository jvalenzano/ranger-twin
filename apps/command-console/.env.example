# MapTiler API Key
# Get yours free at https://cloud.maptiler.com/
VITE_MAPTILER_API_KEY=your_maptiler_api_key_here

# RANGER API Gateway (Skills-First Architecture)
# Set to 'false' to skip API and use direct LLM calls
VITE_USE_RANGER_API=true
VITE_RANGER_API_URL=http://localhost:8000

# OpenRouter API Key (Fallback provider)
VITE_OPENROUTER_API_KEY=your_openrouter_api_key_here

# Google Gemini API Key (For NEPA RAG queries)
# VITE_GEMINI_API_KEY=your_gemini_api_key_here

# Fire Data Source
# true = Real NIFC data (default), false = Mock/simulated data
VITE_USE_REAL_FIRE_DATA=true
